<%- include('partials/header'); -%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <label for="#title">Title</label>
    <input id="title" type="text" name="title" style="width: 100%;" autocomplete="off" />
    <label for="#mess">Post</label>
    <textarea id="mess" name="message" rows="4" cols="50" style="width: 100%;" placeholder="Enter your text here..."></textarea>
    <button class="btn btn-primary" type="submit">Publish</button>
</form>

<%- include('partials/footer'); -%>
